pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
               
               sshagent(['${credos}']) { 
                   sh '''       
                    ssh -o "UserKnownHostsFile=/dev/null"  -o StrictHostKeyChecking=no ruslan@192.168.0.118 <<DEOF

                    DEOF
                    '''.stripIndent()
               }
            }
        }
    }
}

